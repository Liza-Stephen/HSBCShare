package com.hsbc.collections.main;

import java.util.Comparator;

public class SalComparator implements Comparator<Employee>{

	@Override
	public int compare(Employee e1, Employee e2) {
		
		if(e1.getSalary() < e2.getSalary())
			return -1;
		if(e1.getSalary() > e2.getSalary())
			return 1;
		
		rerurn 0;
	}

}
