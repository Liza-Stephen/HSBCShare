		ArrayList<String> lst = new ArrayList<String>();
		lst.add("Chennai");
		lst.add("Hyderabad");
		lst.add("Chennai");
	//	lst.add(300); // converted to lst.add(new Integer(300))
		
		
	lst.remove("Chennai");
	
	lst.add(1,"Delhi");
	
//	Iterator<String> it = lst.iterator();
//	
